{
  "name": "slyboots.github.io",
  "version": "1.0.0",
  "description": "a static website for github pages",
  "main": "index.js",
  "scripts": {
    "cp_sass_partials": "(mkdir _sass/scss || true) && cp -r node_modules/tachyons-sass/scss _sass/scss",
    "cp_sass_main": "cp node_modules/tachyons-sass/tachyons.scss _sass/tachyons.scss",
    "cp_tachyons": "npm run cp_sass_partials && npm run cp_sass_main",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slyboots/slyboots.github.io.git"
  },
  "keywords": [
    "github",
    "blog"
  ],
  "author": "slyboots",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slyboots/slyboots.github.io/issues"
  },
  "homepage": "https://github.com/slyboots/slyboots.github.io#readme",
  "dependencies": {
    "tachyons-sass": "^4.8.1"
  }
}
